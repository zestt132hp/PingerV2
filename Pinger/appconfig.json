{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "AppSettings": {
    "HostFileName": "hosts.json",
    "SectionName": "hosts:Section({0})"
  }
}
